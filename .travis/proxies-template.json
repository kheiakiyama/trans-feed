{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
      "blob-route": {
          "matchCondition": {
              "route": "/"
          },
          "backendUri": "http://transfeedclouddemo.blob.core.windows.net/contents/index.html"
      },
      "blob-about": {
        "matchCondition": {
          "route": "/about"
        },
        "backendUri": "http://transfeedclouddemo.blob.core.windows.net/contents/about/index.html"
      },
      "blob-asserts": {
        "matchCondition": {
            "route": "/assets/{*file}"
        },
        "backendUri": "https://transfeedclouddemo.blob.core.windows.net/contents/assets/{file}"
      },
      "blob-files": {
          "matchCondition": {
              "route": "/{*file}"
          },
          "backendUri": "https://transfeedclouddemo.blob.core.windows.net/contents/{file}.html"
      },
      "letsencrypt": {
          "matchCondition": {
              "route": "/.well-known/acme-challenge/{*rest}"
          },
          "backendUri": "https://%WEBSITE_HOSTNAME%/api/letsencrypt/{rest}"
      }
  }
}